{
  "manifest_version": 3,
  "name": "Call Rhythm",
  "version": "1.0",
  "description": "Loads and runs a remote script from GitHub Pages.",
  "host_permissions": ["https://mrrob100.github.io/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["loader.js"],
      "run_at": "document_idle"
    }
  ]
}
